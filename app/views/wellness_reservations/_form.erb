<%= form_with model: @wellnessreservation, local: true do |f| %>
    <%= f.label :group_size %>   
    <%= f.number_field :group_size %> <br>    

    <%= f.label :date %>  
    <%= f.text_field :date %><br>   

    <%= f.label :time %>    
    <%= f.text_field :time %><br>

    <%= f.label :user_id %>    
    <%= f.collection_select :user_id, User.all, :id, :username %><br>

    <%= f.label :wellnesscenter_id %>    
    <%= f.collection_select :wellness_center_id, WellnessCenter.all, :id, :name %><br>
    

    <%= f.submit "Submit" %>
    <% end %>